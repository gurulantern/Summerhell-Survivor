; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Summerhell Survivor"
run/main_scene="res://Scenes/main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[aseprite_importers]

aseprite_executable_path="C:/Program Files (x86)/Steam/steamapps/common/Aseprite/Aseprite.exe"

[autoload]

UpgradeDb="*res://Utility/UI/upgrade_db.tscn"
SaverLoader="*res://Utility/Saves/saver_loader.gd"
Save="*res://Utility/Saves/save.gd"
SoundManager="*res://addons/sound_manager/sound_manager.gd"
SceneSwitcher="*res://Utility/scene_switcher.gd"

[debug]

file_logging/enable_file_logging=true
settings/stdout/print_gpu_profile=true
settings/stdout/verbose_stdout=true

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Hike"

[editor_plugins]

enabled=PackedStringArray("res://addons/ez_dialogue/plugin.cfg", "res://addons/nklbdev.aseprite_importers/plugin.cfg", "res://addons/sound_manager/plugin.cfg")

[file_customization]

folder_colors={
"res://Art/": "green",
"res://Attack/": "red",
"res://Audio/": "teal",
"res://Enemies/": "orange",
"res://Player/": "yellow",
"res://Scenes/": "blue",
